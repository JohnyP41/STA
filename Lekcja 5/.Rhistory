n.test<- function(x,sigma_zero,alternative="two.sided"){
sigma=c(3)
a=c(0.05)
S<-sum((x-mean(x))^2)/length(x)
statistic<-(length(x)*S)/(sigma_zero^2)
parameter=length(x)-1
method<-c("Test chi-kwadrat w modelu normalnym")
if(sigma^2>sigma_zero^2){
alternative<-"greater"
k<-qpchisq(1-a,parameter)
}
if(sigma^2<sigma_zero^2){
alternative<-"less"
k<-qpchisq(a,parameter)
}
if(sigma^2!=sigma_zero^2){
alternative<-"two.sided"
k<-qpchisq(1-a/2,parameter)
}
b<-list(statistic=statistic,parameter=parameter,alternative=alternative,method=method,k=k)
class(b)<-"htest"
}
